<%= link_to "new", new_regularexam_path %>
<table class="table">
    <% @regulars.each do |regular| %>
        <tr>
            <td><%= regular.ordered %></td>
            <td><%= regular.dayexam %> </td>
            <td><%= regular.timeexam %></td>
            <td><%= regular.yearexam %></td>
            <td><%= regular.semesterexam %></td>
            <td><%= regular.dateexam %></td>
            <td><%= link_to raw("<i class='glyphicon glyphicon-edit'></i>"), edit_regularexam_path(regular) %></td>
            <td><%= link_to "delete", regularexam_path(regular), method: :delete, data: { confirm: "DELETE?" } %></td>
        </tr>
    <% end %>
</table>
<hr>

